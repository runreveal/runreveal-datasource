{
  "name": "run-reveal",
  "version": "0.1.4",
  "description": "A grafana data source plugin that allows you to access you RunReveal logs and write queries for keeping track of specific metrics",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn run lint --fix",
    "e2e": "yarn exec cypress install && yarn exec grafana-e2e run",
    "e2e:update": "yarn exec cypress install && yarn exec grafana-e2e run --update-screenshots",
    "server": "docker-compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest"
  },
  "author": "RunReveal",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@grafana/eslint-config": "^5.1.0",
    "@grafana/tsconfig": "^1.2.0-rc1",
    "@swc/core": "^1.3.51",
    "@swc/helpers": "^0.5.0",
    "@swc/jest": "^0.2.26",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.194",
    "@types/node": "^20.2.5",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "glob": "^10.1.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.8.7",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.62.1",
    "sass-loader": "13.3.1",
    "style-loader": "3.3.3",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.1.3",
    "webpack": "^5.79.0",
    "webpack-cli": "^5.0.1",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@emotion/css": "^11.1.3",
    "@grafana/data": "^10.0.0",
    "@grafana/runtime": "^10.0.0",
    "@grafana/ui": "^10.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tslib": "2.5.3"
  },
  "packageManager": "yarn@1.22.19"
}
